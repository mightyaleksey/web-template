{
  "dependencies": {
    "preact": "10.24.0"
  },
  "devDependencies": {
    "@preact/preset-vite": "2.9.1"
  },
  "standard": {
    "envs": [
      "browser"
    ],
    "globals": [
      "$Diff",
      "$Exact",
      "$Exports",
      "$KeyMirror",
      "$Keys",
      "$NonMaybeType",
      "$ReadOnlyArray",
      "$ReadOnly",
      "$Rest",
      "$TupleMap",
      "$Values"
    ],
    "parser": "hermes-eslint",
    "parserOptions": {
      "sourceType": "module"
    }
  }
}
